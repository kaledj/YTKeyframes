from pytube import YouTube
from pprint import pprint
import sys
import mplayer


yt = YouTube()

yt.url = sys.argv[1]
# print yt.url

video = yt.get('mp4', '720p')
video = yt.get('mp4')
video.download()

p = mplayer.Player(args=(), stdout=mplayer.PIPE, stderr=None, autospawn=True)
p.loadfile(video)

print yt.filename